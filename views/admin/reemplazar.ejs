<%- include(partials + 'header.ejs' ) %>

            <div id="content">
                <div class="container-fluid">
                    <div class="col-lg-12 text-center">
                        <div class="d-sm-flex align-items-center justify-content-md-center mb-4" data-title>
                            <h1 class="h3 mb-0 text-gray-800">Piezas a reemplazar
                            </h1>
                        </div>
                    </div>

                    <div class="row col-md-12 mb-2 align-items-center">
                        <div class="d-flex justify-content-center align-items-center" data-loading></div>
                    </div>

                    <div class="row justify-content-md-center">
                        <div class="col-lg-3">
                            <div class="card shadow mb-3">
                                <form data-submit-item enctype="multipart/form-data">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-danger">Reemplazar nueva pieza</h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="form-group">
                                            <input id="km" name="km" placeholder="Inserte el KM atual del vehiculo *"
                                                type="number" class="form-control" required>
                                        </div>
                                        <div class="form-group">
                                            <input id="code" name="code" placeholder="Codigo da Pieza " type="text"
                                                class="form-control" style="text-transform:uppercase">
                                        </div>
                                        <div class="form-group">
                                            <input id="item" name="item" placeholder="Pieza a ser reemplazada *"
                                                type="text" class="form-control" style="text-transform:uppercase"
                                                required>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="custom-file">
                                                    <input id="file" name="file" type="file" multiple
                                                        class="custom-file-input">
                                                    <label class="custom-file-label" for="inputGroupFile01">Foto de la
                                                        Pieza</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <input id="obs" name="obs" placeholder="ObservaciÃ³n" type="text"
                                                class="form-control" style="text-transform:uppercase">
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group mb-3">
                                                <div class="input-group-prepend">
                                                    <button data-modal-insert class="btn btn-outline-success"
                                                        type="button">Agregar nuevo</button>
                                                </div>
                                                <select id="provider" name="provider" type="text" class="form-control"
                                                    data-providers>
                                                    <option value="" selected hidden>Proveedor *
                                                    </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <input id="brand" name="brand" placeholder="Marca de la pieza " type="text"
                                                class="form-control" style="text-transform:uppercase" disabled>
                                        </div>
                                        <div class="form-group">
                                            <input id="amount" name="amount" placeholder="Cant" min="0" type="number"
                                                class="form-control" disabled>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group mb-3">
                                                <div class="input-group-prepend">
                                                    <select data-currency id="currency" name="currency" type="text"
                                                        class="btn btn-outline-success" data-providers disabled>
                                                        <option value="1" selected>GS</option>
                                                        <option value="2">USD</option>
                                                    </select>
                                                </div>
                                                <input id="price" name="price" placeholder="Precio" type="text"
                                                    value="0000" class="form-control" disabled>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="custom-file">
                                                    <input id="voucher" name="voucher" type="file"
                                                        class="custom-file-input" disabled>
                                                    <label class="custom-file-label" for="inputGroupFile01">Foto de lo
                                                        presupuesto</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <select id="type" name="type" type="text" class="form-control" disabled>
                                                <option value="" selected hidden="hidden">Origen de la nueva pieza *
                                                </option>
                                                <option value="1">Presupuesto</option>
                                                <option value="2">Stock</option>
                                            </select>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-12 text-right">
                                                <button data-button-submit type="submit"
                                                    class="btn btn-success">Agregar</button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                        <div class="col-lg-9">
                            <div class="card shadow mb-4">
                                <div class="card-header py-3">
                                    <h6 class="m-0 font-weight-bold text-danger">Tabla del Piezas</h6>
                                </div>
                                <div class="card-body">
                                    <div class="form-group">
                                        <select data-cars type="text" class="form-control">
                                            <option value="" selected disabled>Seleccione la chapa del vehiculo *
                                            </option>
                                        </select>
                                        <small id="passwordHelpBlock" class="form-text text-muted">
                                            Listado de Veiculos origen excel del cadastro vehicular.
                                        </small>
                                    </div>
                                    <table class="table table-bordered text-center" id="dataTable" width="100%"
                                        cellspacing="0" data-table></table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <%- include(partials + 'script.ejs' ) %>

                <script type="module" src="../js/controllers/maintenanceController.js"></script>
                <script type="module" src="../js/controllers/loginController.js"></script>
                <script type="module" src="../js/controllers/menuController.js"></script>

                <%- include(partials + 'footer.ejs' ) %>